# ALIAS
alias ll='ls -l'
alias la='ls -a'
alias ..='cd ..'
alias ...='cd ../..' 
alias commit='git commit -m'
alias commitall='git add . && commit'
alias commitm='bash ~/my_scripts/commit-multiple-messages.sh'
alias push='bash ~/my_scripts/push-current-git-branch.sh'
alias pull='bash ~/my_scripts/pull-current-git-branch.sh'
alias pullm='bash ~/my_scripts/pull-from-main.sh'
alias status='git status'
alias new-branch='git checkout -b'

# Redirect to apps and start server MAC
alias money-app='cd Projects/money-app/ && yarn dev'

# Redirect to apps and start server WINDOWS
